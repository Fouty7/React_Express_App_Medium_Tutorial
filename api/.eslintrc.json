{
    "env": {
      "es2021": true,
      "node": true,
      "mocha": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "plugins": [
      "node",
      "import"
    ],
    "rules": {
      "node/no-unsupported-features/es-syntax": ["error", { "version": ">=14" }],
      "node/no-unpublished-require": ["error", { "allowModules": ["dotenv","supertest"] }]
    }
  }
  